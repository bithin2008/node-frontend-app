{
  "name": "fphw-website",
  "version": "2.11.101",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --o",
    "prebuild": "npm --no-git-tag-version version patch",
    "build": "ng build --aot",
    "postbuild": "git add . && git commit -m 'new build' && git push",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean": "rm -rf dist node_modules",
    "dev:ssr": "ng run fphw-website:serve-ssr:staging",
    "serve:ssr": "node dist/fphw-website/server/main.js",
    "build:ssr": "ng build --configuration=production && ng run fphw-website:server:production",    
    "prerender": "ng run fphw-website:prerender"
  },
